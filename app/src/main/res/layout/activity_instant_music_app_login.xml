<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/grade"
    tools:context=".MusicApp">

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="visible">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:orientation="horizontal"
        android:visibility="visible">

        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/secondaryColor"
            tools:visibility="visible" />

        <ImageView
            android:id="@+id/menuButton15"
            style="@android:style/ButtonBar"
            android:layout_width="80sp"
            android:layout_height="70sp"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />

        <ImageView
            android:id="@+id/imageView18"
            android:layout_width="2sp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />

        <ImageView
            android:id="@+id/menuButton16"
            style="@android:style/ButtonBar"
            android:layout_width="80sp"
            android:layout_height="70sp"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />

        <ImageView
            android:id="@+id/imageView19"
            android:layout_width="2sp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />

        <ImageView
            android:id="@+id/menuButton17"
            style="@android:style/ButtonBar"
            android:layout_width="80sp"
            android:layout_height="70sp"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />

        <ImageView
            android:id="@+id/imageView20"
            android:layout_width="2sp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />

        <ImageView
            android:id="@+id/menuButton18"
            style="@android:style/ButtonBar"
            android:layout_width="80sp"
            android:layout_height="70sp"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />

        <ImageView
            android:id="@+id/imageView21"
            android:layout_width="2sp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />

        <ImageView
            android:id="@+id/menuButton19"
            style="@android:style/ButtonBar"
            android:layout_width="80sp"
            android:layout_height="70sp"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />

        <ImageView
            android:id="@+id/imageView22"
            android:layout_width="2sp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="bottom"
        android:orientation="vertical"
        app:layout_anchor="@+id/linearLayout6"
        app:layout_anchorGravity="center">

        <LinearLayout
            android:id="@+id/linearLayout5"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="bottom"
            android:orientation="horizontal"
            android:visibility="visible">

            <Space
                android:layout_width="20sp"
                android:layout_height="wrap_content"
                android:layout_weight="0" />

            <Button
                android:id="@+id/menuButton1"
                style="@android:style/ButtonBar"
                android:layout_width="50sp"
                android:layout_height="50sp"
                android:layout_weight="0"
                android:background="@drawable/home480_2" />

            <Space
                android:layout_width="30sp"
                android:layout_height="match_parent"
                android:layout_weight="0" />

            <Button
                android:id="@+id/menuButton2"
                style="@android:style/ButtonBar"
                android:layout_width="50sp"
                android:layout_height="50sp"
                android:layout_weight="0"
                android:background="@drawable/searchicon" />

            <Space
                android:layout_width="35sp"
                android:layout_height="wrap_content"
                android:layout_weight="0" />

            <Button
                android:id="@+id/menuButton3"
                android:layout_width="40sp"
                android:layout_height="40sp"
                android:layout_weight="0"
                android:background="@drawable/microphone100" />

            <Space
                android:layout_width="40sp"
                android:layout_height="wrap_content"
                android:layout_weight="0" />

            <Button
                android:id="@+id/menuButton4"
                android:layout_width="40sp"
                android:layout_height="40sp"
                android:layout_weight="0"
                android:background="@drawable/friends100" />

            <Space
                android:layout_width="50sp"
                android:layout_height="wrap_content"
                android:layout_weight="0" />

            <Button
                android:id="@+id/menuButton5"
                android:layout_width="40sp"
                android:layout_height="40sp"
                android:layout_weight="0"
                android:background="@drawable/settings100" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:layout_gravity="bottom"
            android:orientation="horizontal">

            <Space
                android:layout_width="25sp"
                android:layout_height="wrap_content"
                android:layout_weight="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:text="@string/home"
                android:textAlignment="center"
                android:textColor="@color/color2"
                tools:text="Home" />

            <Space
                android:layout_width="40sp"
                android:layout_height="match_parent"
                android:layout_weight="0" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:text="@string/searchButton"
                android:textAlignment="center"
                android:textColor="@color/color2"
                tools:text="Search" />

            <Space
                android:layout_width="27sp"
                android:layout_height="wrap_content"
                android:layout_weight="0" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:text="@string/podcasts"
                android:textAlignment="center"
                android:textColor="@color/color2"
                tools:text="Podcasts" />

            <Space
                android:layout_width="30sp"
                android:layout_height="wrap_content"
                android:layout_weight="0" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:text="@string/friends"
                android:textAlignment="center"
                android:textColor="@color/color2"
                tools:text="Friends" />

            <Space
                android:layout_width="40sp"
                android:layout_height="wrap_content"
                android:layout_weight="0" />

            <TextView
                android:id="@+id/textView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="0"
                android:text="@string/settings"
                android:textAlignment="center"
                android:textColor="@color/color2"
                tools:text="Settings" />
        </LinearLayout>

    </LinearLayout>


</androidx.coordinatorlayout.widget.CoordinatorLayout>