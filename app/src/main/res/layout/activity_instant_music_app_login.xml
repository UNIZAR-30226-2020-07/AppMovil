<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/grade"
    tools:context=".MusicApp">

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:visibility="visible">

    </LinearLayout>

    <LinearLayout
        android:id="@+id/linearLayout6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:orientation="horizontal"
        android:visibility="visible">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="2dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/secondaryColor"
            tools:visibility="visible" />

        <ImageView
            android:id="@+id/menuButton"
            style="@android:style/ButtonBar"
            android:layout_width="80sp"
            android:layout_height="70sp"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="2sp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />

        <ImageView
            android:id="@+id/menuButton11"
            style="@android:style/ButtonBar"
            android:layout_width="80sp"
            android:layout_height="70sp"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="2sp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />

        <ImageView
            android:id="@+id/menuButton12"
            style="@android:style/ButtonBar"
            android:layout_width="80sp"
            android:layout_height="70sp"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="2sp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />

        <ImageView
            android:id="@+id/menuButton13"
            style="@android:style/ButtonBar"
            android:layout_width="80sp"
            android:layout_height="70sp"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="2sp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />

        <ImageView
            android:id="@+id/menuButton14"
            style="@android:style/ButtonBar"
            android:layout_width="80sp"
            android:layout_height="70sp"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="2sp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/secondaryColor" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="bottom"
        android:orientation="vertical"
        app:layout_anchor="@+id/linearLayout6"
        app:layout_anchorGravity="center">

        <LinearLayout
            android:id="@+id/linearLayout5"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="bottom"
            android:orientation="horizontal"
            android:visibility="visible">

            <Space
                android:layout_width="20dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <Button
                android:id="@+id/menuButton1"
                style="@android:style/ButtonBar"
                android:layout_width="45dp"
                android:layout_height="45dp"
                android:layout_weight="0"
                android:background="@drawable/home480_2" />

            <Space
                android:layout_width="45dp"
                android:layout_height="match_parent"
                android:layout_weight="1" />

            <Button
                android:id="@+id/menuButton2"
                style="@android:style/ButtonBar"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_weight="0"
                android:background="@drawable/lupagrande" />

            <Space
                android:layout_width="40dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <Button
                android:id="@+id/menuButton3"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_weight="0"
                android:background="@drawable/microphone100" />

            <Space
                android:layout_width="40dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <Button
                android:id="@+id/menuButton4"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_weight="0"
                android:background="@drawable/friends100" />

            <Space
                android:layout_width="40dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <Button
                android:id="@+id/menuButton5"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_weight="0"
                android:background="@drawable/settings100" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_gravity="bottom"
            android:orientation="horizontal">

            <Space
                android:layout_width="27dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="50dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/home"
                android:textAlignment="center"
                android:textColor="@color/color2"
                tools:text="Home" />

            <Space
                android:layout_width="47dp"
                android:layout_height="match_parent"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="62dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/searchButton"
                android:textAlignment="center"
                android:textColor="@color/color2"
                tools:text="Search" />

            <Space
                android:layout_width="28dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textView17"
                android:layout_width="73dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/podcasts"
                android:textAlignment="center"
                android:textColor="@color/color2"
                tools:text="Podcasts" />

            <Space
                android:layout_width="29dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="58dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/friends"
                android:textAlignment="center"
                android:textColor="@color/color2"
                tools:text="Friends" />

            <Space
                android:layout_width="33dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

            <TextView
                android:id="@+id/textView19"
                android:layout_width="65dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:text="@string/settings"
                android:textAlignment="center"
                android:textColor="@color/color2"
                tools:text="Settings" />

            <Space
                android:layout_width="20dp"
                android:layout_height="wrap_content"
                android:layout_weight="1" />

        </LinearLayout>

    </LinearLayout>


</androidx.coordinatorlayout.widget.CoordinatorLayout>